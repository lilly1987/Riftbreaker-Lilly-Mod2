import * from "materials/scripts/buildings_resources.material"

material buildings/resources/bio_composter_lvl_2 : buildings/destructible
{
	set_texture_alias albedo			materials/textures/buildings/resources/bio_composter_lvl_2_albedo.dds
	set_texture_alias normal			materials/textures/buildings/resources/bio_composter_normal.dds
	set_texture_alias packed			materials/textures/buildings/resources/bio_composter_packed.dds
	set_texture_alias emissive			materials/textures/buildings/resources/bio_composter_emissive.dds
	technique 0
	{
		pass 0
		{
			glow 13
        }
    }
}

